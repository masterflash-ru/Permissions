{
    "name": "masterflash-ru/permissions",
    "description": "Выделеный сервис контроля доступа к ресурсам Simba как в UNIX и сервис авторизации",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zendframework/zend-permissions-acl": "^2.6",
        "masterflash-ru/ado": "^2.0",
        "masterflash-ru/users": "*"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Permissions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Permissions\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Permissions",
            "config-provider": "masterflash-ru\\Mf\\Permissions\\ConfigProvider"
        }
    }
}
